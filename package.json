{
  "name": "game-store-react",
  "version": "1.0.0",
  "description": "Game Store built with ReactJS",
  "main": "index.js",
  "scripts": {
    "start2": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "echo \"Error: no test specified\"",
    "dev": "npm run client",
    "start": "node server.js",
    "postinstall": "npm run build",
    "build": "NODE_ENV=production webpack --env production ",
    "build2": "SUBDIR=https://jojozhuang.github.io/game-store-react/ NODE_ENV=production webpack --env production ",
    "client": "PORT=12090 webpack-dev-server --env development --devtool inline-source-map --hot"
  },
  "keywords": [
    "react"
  ],
  "author": {
    "name": "Rong Zhuang",
    "email": "jojozhuang@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "braces": "^3.0.2",
    "express": "^4.17.1",
    "mem": "^6.0.1",
    "react": "^16.8.6",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.8.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^5.1.0",
    "eslint": "^5.16.0",
